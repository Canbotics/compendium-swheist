		<% detailRequest.order.forEach((battle) => { %><article id="<%= battle.uri %>" class="list entry battle">
			<div>
				<header>
					<a href="<%= detailPage.uri[detailPage.lang] + "/" + battle.uri %>"><h2><%= battle.name %></h2></a>
					<span class="<%= battle.region[1] %>"><%= battle.region[0] %></span>
					<% if(battle.challenge) { %><img src="<%= dataSite.uri.rootAsset %>steamworld-heist/stages/decal-stage-challenge.png" width="30" height="33" alt="<%= dataSite[detailPage.lang].term.stageBattleChallenge %>"><% } %>
				</header>
				<ul>
					<li><img src="<%= dataSite.uri.rootAsset %>steamworld-heist/global/decal-epic-loot-<%= battle.attr[0] %>.png" width="<%= battle.attr[0] * 74 %>" height="77" <% if (battle.attr[0] == 1) { %>title="<%= dataSite[detailPage.lang].term.stageBattleEpic[0] %>" alt="<%= dataSite[detailPage.lang].term.stageBattleEpic[0] %>"<% } else { %>title="<%= dataSite[detailPage.lang].term.stageBattleEpic[1].replace("[NUMBER]",battle.attr[0]) %>" alt="<%= dataSite[detailPage.lang].term.stageBattleEpic[1].replace("[NUMBER]",battle.attr[0]) %>"<% } %>></li>
					<li><img src="<%= dataSite.uri.rootAsset %>steamworld-heist/global/decal-character-<%= battle.attr[1] %>.png" width="<%= battle.attr[1] * 74 %>" height="77" <% if (battle.attr[1] == 1) { %>title="<%= dataSite[detailPage.lang].term.stageBattleChar[0] %>" alt="<%= dataSite[detailPage.lang].term.stageBattleChar[0] %>"<% } else { %>title="<%= dataSite[detailPage.lang].term.stageBattleChar[1].replace("[NUMBER]",battle.attr[1]) %>" alt="<%= dataSite[detailPage.lang].term.stageBattleChar[1].replace("[NUMBER]",battle.attr[1]) %>"<% } %>></li>
					<li><img src="<%= dataSite.uri.rootAsset %>steamworld-heist/global/decal-star-<%= battle.attr[2] %>.png" width="<%= battle.attr[2] * 74 %>" height="77" <% if (battle.attr[2] == 1) { %>title="<%= dataSite[detailPage.lang].term.stageBattleStar[0] %>" alt="<%= dataSite[detailPage.lang].term.stageBattleStar[0] %>"<% } else { %>title="<%= dataSite[detailPage.lang].term.stageBattleStar[1].replace("[NUMBER]",battle.attr[2]) %>" alt="<%= dataSite[detailPage.lang].term.stageBattleStar[1].replace("[NUMBER]",battle.attr[2]) %>"<% } %>></li>
				</ul>
				<div>
					<h3 class="vishide"><%= dataSite[detailPage.lang].term.genDesc %></h3>
					<p><%= battle.desc %></p>
				</div>
			</div>
			<img src="<%= dataSite.uri.rootAsset %>steamworld-heist/stages/<%= battle.asset %>" width="169" height="169" alt="">
		</article><% }) %>